{
  "name": "@ramdesh/knowledgelab",
  "version": "0.0.2",
  "description": "A JupyterLab extension for knowledge-repo integration",
  "author": "Tim Paine, Ramindu Deshapriya",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublish": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{ts}"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.css"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.2",
    "@jupyterlab/apputils": "^1.0.2",
    "@jupyterlab/docmanager": "^1.0.2",
    "@jupyterlab/docregistry": "^1.0.2",
    "@jupyterlab/notebook": "^1.0.2",
    "@jupyterlab/rendermime": "^1.0.2",
    "@phosphor/disposable": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-transform-css": "^2.0.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
